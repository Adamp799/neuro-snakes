    "import gym\n",
    "import gym_snake\n",
    "\n",
    "env = gym.make('snake-v0')\n",
    "obs = env.reset()\n",
    "done = False\n",
    "env.render()\n",
    "while not done:\n",
    "    act = env.action_space.sample()\n",
    "    obs, reward, done, info = env.step(act)\n",
    "    env.render()\n",
    "env.render(close=True)"
   ]
  }
